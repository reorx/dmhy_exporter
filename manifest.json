{
  "name": "DMHY Exporter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Export magnets on DMHY",
  "homepage_url": "https://github.com",
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "tabs",
    "https://share.dmhy.org/*",
    "http://share.dmhy.org/*"
  ],
  "web_accessible_resources": [
    "main.js",
    "clipboard.min.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://share.dmhy.org/*",
        "http://share.dmhy.org/*"
      ],
      "css": ["style.css"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}